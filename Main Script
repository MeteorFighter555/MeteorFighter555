  FishTab:AddButton({
	Name = "Fisch Gui",
	Callback = function(value)
	        setclipboard("FischNew87923!GoodRoblox")
         toclipboard("FischNew87923!GoodRoblox")
		        loadstring(game:HttpGet("https://raw.githubusercontent.com/LOLking123456/Fisch/refs/heads/main/New"))()
  	end    
})
	
	local UtilTab = Window:MakeTab({
	Name = "Utility",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
	
UtilTab:AddButton({
	Name = "Click TP",
	Callback = function()
      		mouse = game.Players.LocalPlayer:GetMouse()
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Click Teleport"
tool.Activated:connect(function()
local pos = mouse.Hit+Vector3.new(0,2.5,0)
pos = CFrame.new(pos.X,pos.Y,pos.Z)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
end)
tool.Parent = game.Players.LocalPlayer.Backpack
  	end    
})

UtilTab:AddButton({
	Name = "Parkour",
	Callback = function()
loadstring(game:HttpGet(('https://pastefy.app/6HAb8rAj/raw'),true))()
  end
})

